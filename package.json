{
  "name": "jangouts",
  "version": "0.4.4",
  "license": "MIT",
  "description": "Videoconferencing based on WebRTC and Janus Gateway with an UI inspired by Google Hangouts",
  "homepage": "https://github.com/jangouts/jangouts",
  "scripts": {
    "postinstall": "npm run bower -- install && npm run typings -- install",
    "server": "npm run server:dev",
    "server:dev": "npm run webpack-dev-server -- --watch --inline --progress --port 4000",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors -p 4000",
    "clean": "npm cache clean && npm run rimraf -- node_modules bower_components doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run webpack -- --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run webpack -- --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint 'src/**/*.ts' || true",
    "test": "$(npm bin)/karma start --single-run || true",
    "test:watch": "npm run karma start -- --auto-watch",
    "ci": "npm test",
    "rimraf": "$(npm bin)/rimraf",
    "karma": "$(npm bin)/karma",
    "tslint": "$(npm bin)/tslint",
    "typedoc": "($npm bin)/typedoc",
    "typings": "$(npm bin)/typings",
    "webpack": "$(npm bin)/webpack",
    "webpack-dev-server": "$(npm bin)/webpack-dev-server",
    "webdriver-manager": "$(npm bin)/webdriver-manager",
    "protractor": "$(npm bin)/protractor",
    "bower": "bower"
  },
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@angular/upgrade": "~2.4.0",
    "angular2-modal": "~2.0.2",
    "core-js": "^2.4.0",
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mousetrap": "^1.5.3",
    "rxjs": "~5.0.1",
    "zone.js": "~0.7.2"
  },
  "devDependencies": {
    "codelyzer": "0.0.20",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "^0.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "ng-embed": "^2.0.4",
    "node-sass": "^3.7.0",
    "parse5": "^1.5.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^4.0.14",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "1.1.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.3",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.3.12",
    "typescript": "~2.0.0",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jangouts/jangouts/.git"
  },
  "bugs": {
    "url": "https://github.com/jangouts/jangouts/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
